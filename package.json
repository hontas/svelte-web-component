{
  "name": "svelte-wc",
  "version": "0.0.0",
  "description": "Webcomponent svelte-wc following open-wc recommendations",
  "author": "svelte-wc",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config",
    "lint": "eslint . --ignore-path .gitignore --ext .js,.svelte",
    "generateDocs": "node script/generateDocs.js",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "storybook": "start-storybook -s dist",
    "storybook:build": "build-storybook"
  },
  "devDependencies": {
    "@babel/types": "^7.10.4",
    "@open-wc/eslint-config": "^2.0.6",
    "@open-wc/testing": "^2.5.19",
    "@open-wc/testing-karma": "^4.0.9",
    "@storybook/addon-essentials": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@storybook/theming": "^5.3.19",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "doctrine": "^3.0.0",
    "doiuse": "^4.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-svelte-wc": "file:./eslint-plugin-svelte-wc",
    "eslint-plugin-svelte3": "^2.7.3",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss": "^7.0.32",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.21.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.24.0",
    "svelte-loader": "^2.13.6",
    "sveltedoc-parser": "^2.3.4",
    "webpack-merge": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
